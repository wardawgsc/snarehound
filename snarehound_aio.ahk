ShowLocation() {
    global lastCoords

    scTitle := "Star Citizen"
    if !WinExist(scTitle) {
        ; Silent fail or minimal notification as requested
        return
    }

    WinActivate(scTitle)
    if !WinWaitActive(scTitle, , 2) {
        return
    }

    ; Save current clipboard
    oldClip := ClipboardAll()
    A_Clipboard := ""
    
    Sleep(100)
    Send("{Enter}") ; Open Chat
    Sleep(100)

    ; Typing command character-by-character as in functioning backup
    for char in StrSplit("/showlocation") {
        Send(char)
        Sleep(20)
    }
    
    Sleep(100)
    Send("{Enter}") ; Execute command

    ; Wait for coordinates to hit clipboard
    if !ClipWait(2) {
        A_Clipboard := oldClip
        return
    }

    lastCoords := A_Clipboard

    ; Cleanup chat window
    Sleep(150)
    Send("{Enter}") ; Close chat if still open
    Sleep(150)
    Send("{Escape}")
    Sleep(150)
    Send("{Escape}")
}

ShowTimeline() {
    global catchHistory
    if catchHistory.Length = 0 {
        MsgBox("No tactical events recorded.", "HUD Archive")
        return
    }
    timeline := "RSI TACTICAL HUD ARCHIVE:`n`n"
    for i, entry in catchHistory {
        if (entry.ship = "UNKNOWN")
            continue
        timeline .= "[ " entry.time " ] " entry.ship "`n"
    }
    MsgBox(timeline, "HUD Archive")
}

ResetSession() {
    global triggerTimes, catchCount, catchHistory, lastTriggerLine, lastSize
    if MsgBox("Reset all session data?", "Confirm Reset", "YesNo") = "No"
        return
    
    triggerTimes := []
    catchCount := 0
    catchHistory := []
    targetHistory := [] ; Clear rolling target list
    lastTriggerLine := ""
    
    snareGui["LastTrigger"].Text := "LOG_LAST: NONE"
    snareGui["CatchInterval"].Text := "INTV_AVG: N/A"
    snareGui["CatchCount"].Text := "TOTAL_CATCH: 0"
    Loop 5 {
        snareGui["Target" A_Index].Text := "T" A_Index ": NONE"
        snareGui["Target" A_Index].SetFont("c" colorGrey)
    }
    snareGui["LogDisplay"].Value := "Waiting for signatures..."
    
    ToolTip("DATA_CLEARED", 100, 100)
    SetTimer(() => ToolTip(), -2000)
}

UpdateCatchInterval() {
    global triggerTimes, snareGui

    if triggerTimes.Length < 2 {
        snareGui["CatchInterval"].Text := "INTV_AVG: N/A"
        return
    }

    total := 0
    for i, t in triggerTimes {
        if i > 1
            total += t - triggerTimes[i - 1]
    }

    avgMs := total // (triggerTimes.Length - 1)
    avgSec := Round(avgMs / 1000)
    min := Floor(avgSec / 60)
    sec := Mod(avgSec, 60)

    snareGui["CatchInterval"].Text := "[ INTV_AVG: " min "m " sec "s ]"
}

UpdateTargetUI(shipName) {
    global targetHistory, snareGui, colorGold
    
    ; Filtering: Don't show UNKNOWNs in the rolling target list
    if (shipName = "UNKNOWN")
        return
        
    ; Add to rolling history (max 5)
    targetHistory.InsertAt(1, shipName)
    if (targetHistory.Length > 5)
        targetHistory.Pop()
        
    ; Update Labels (Ship name only, no timestamp)
    Loop 5 {
        label := "Target" A_Index
        if (A_Index <= targetHistory.Length) {
            name := targetHistory[A_Index]
            snareGui[label].Text := "T" A_Index ": " StrUpper(name)
            snareGui[label].SetFont("c" colorGold)
        } else {
            name := "NONE"
            snareGui[label].Text := "T" A_Index ": NONE"
            snareGui[label].SetFont("c" colorGrey)
        }
    }
}

WatchLog() {
    global logFile, triggerText, lastSize, lastTriggerLine, triggerTimes, recentLogLines, snareGui, catchCount, catchHistory
    global signatureBuffer, lastLogTime, knownMappings, shipTrackerKeywords

    if !FileExist(logFile) {
        snareGui["StatusText"].SetFont("cYellow")
        snareGui["StatusText"].Text := "LOG_FILE_MISSING"
        return
    }

    try {
        fh := FileOpen(logFile, "r")
        if !fh
            return

        fh.Seek(0, 2)
        newSize := fh.Pos

        if (newSize < lastSize) {
            lastSize := 0
        }

        if (newSize > lastSize) {
            fh.Seek(lastSize)
            newText := fh.Read(newSize - lastSize)
            lastSize := newSize

            lines := StrSplit(newText, "`n", "`r")
            for i, line in lines {
                if (line = "")
                    continue
                
                ; Look for trigger text
                if InStr(line, triggerText) {
                    PlayAlertSound()
                    lastTriggerLine := line
                    triggerTimes.Push(A_TickCount)
                    catchCount++
                    timeStr := FormatTime(A_Now, "hh:mm:ss tt")
                    catchHistory.Push({time: timeStr, ship: "UNKNOWN"})
                    UpdateCatchInterval()
                    snareGui["LastTrigger"].Text := "[ LOG_LAST: " timeStr " ]"
                    snareGui["CatchCount"].Text := "[ TOTAL_CATCH: " catchCount " ]"
                    break ; Only one sound per log read chunk (matching backup logic)
                }

                ; Ship Tracking logic - uses expanded keywords
                isRelevant := false
                for kw in shipTrackerKeywords {
                    if InStr(line, kw) {
                        isRelevant := true
                        break
                    }
                }
                if (isRelevant) {
                    signatureBuffer.Push(Trim(line))
                    lastLogTime := A_TickCount
                }
            }
        }
        fh.Close()

        ; Flush buffer if no new lines for 1.5 seconds
        if (signatureBuffer.Length > 0 && (A_TickCount - lastLogTime > 1500)) {
            ; FILTER: Ignore "Failed to get starmap route data!" and anything after it
            newBuffer := []
            for line in signatureBuffer {
                if InStr(line, "Failed to get starmap route data!")
                    break
                newBuffer.Push(line)
            }
            signatureBuffer := newBuffer

            if (signatureBuffer.Length = 0) {
                signatureBuffer := []
                return
            }

            normSig := NormalizeSignature(signatureBuffer)
            
            if knownMappings.Has(normSig) {
                shipName := knownMappings[normSig]
                UpdateTargetUI(shipName)

                ; Update history with identified ship
                if (catchHistory.Length > 0)
                    catchHistory[catchHistory.Length].ship := shipName
            } else {
                ; Unknown signature! 
                UpdateTargetUI("UNKNOWN")
            }
            
            signatureBuffer := []
        }

        if (snareGui["StatusText"].Text = "LOG_FILE_MISSING") {
            snareGui["StatusText"].SetFont("c" colorCyan)
            snareGui["StatusText"].Text := "[ STATUS // ACTIVE ]"
        }
    } catch as e {
        ; Silent
    }
}
